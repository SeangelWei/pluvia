package model;

import utils.GameObject;
import utils.Pluvia;

public class Shot extends GameObject {
    int speed;
    float birthX;
    float birthY;

    public Shot(float x, float y) {
        super(x, y-380);
        speed = 300;
    }

    @Override
    public void update() {
        position.y += Pluvia.delta*speed;
    }
}
